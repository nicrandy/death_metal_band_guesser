{
  "name": "my-app",
  "version": "0.1.0",
  "homepage": "https://nicrandy.github.io/death_metal_band_guesser",
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "seed": "esbuild prisma/seed.ts --outfile=node_modules/tmp-seed.cjs --bundle --format=cjs --external:prisma --external:@prisma/client && node node_modules/tmp-seed.cjs --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "isbot": "^3.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "esbuild": "^0.15.11",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "gh-pages": "^4.0.0",
    "prisma": "^4.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
